# UrbanForm Pro - Environment Variables Template
# Copy this file to .env and fill in your actual values

# ==============================================
# MapTiler Configuration (Required)
# ==============================================
# Get your free API key at: https://cloud.maptiler.com/account/keys/
REACT_APP_MAPTILER_KEY=your_maptiler_api_key_here

# ==============================================
# Backend API Configuration
# ==============================================
# Local development
REACT_APP_API_URL=http://localhost:5000/api

# Production (update after deploying backend)
# REACT_APP_API_URL=https://your-backend.onrender.com/api

# ==============================================
# Feature Flags
# ==============================================
REACT_APP_ENABLE_3D=true
REACT_APP_ENABLE_ML=true
REACT_APP_ENABLE_ANALYTICS=false

# ==============================================
# Analytics (Optional)
# ==============================================
REACT_APP_GA_TRACKING_ID=
REACT_APP_SENTRY_DSN=

# ==============================================
# Application Settings
# ==============================================
REACT_APP_VERSION=2.0.0
REACT_APP_ENV=development

# ==============================================
# Development Settings
# ==============================================
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=info
