# ============================================
# UrbanForm Pro - Backend Environment Variables
# ============================================
# Copy this file to .env and fill in your actual API keys

# ==============================================
# MapTiler API Configuration (Required)
# ==============================================
# Get your free API key at: https://cloud.maptiler.com/account/keys/
MAPTILER_KEY=your_maptiler_api_key_here

# ==============================================
# Geoapify API Configuration (Required)
# ==============================================
# Get your free API key at: https://www.geoapify.com/
GEOAPIFY_KEY=your_geoapify_api_key_here

# ==============================================
# Application Settings
# ==============================================
FLASK_ENV=development
FLASK_DEBUG=True
PORT=5000

# ==============================================
# Security (For Production)
# ==============================================
# SECRET_KEY=your_secret_key_here
# CORS_ORIGINS=https://your-frontend-domain.com
